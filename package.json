    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/platform-socket.io": "^9.0.11",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/typeorm": "^8.0.2",
    "@nestjs/websockets": "^9.0.11",
    "@prisma/client": "^4.3.1",
