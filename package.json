    "@types/socket.io": "^3.0.2",
    "apollo-server-express": "^3.10.2",
    "aws-sdk": "^2.1204.0",
    "bcrypt": "^5.0.1",
    "cache-manager": "^4.1.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
